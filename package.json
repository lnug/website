{
  "name": "lnug.github.io",
  "version": "1.0.0",
  "description": "lnug.github.io",
  "main": "build.js",
  "scripts": {
    "test": "standard && mocha --recursive test",
    "update": "node ./bin/update-json.js && npm run fetch-markdown",
    "build-markup": "node ./bin/build-markup.js",
    "fetch-markdown": "node ./bin/fetch-markdown.js",
    "photos": "node ./bin/photos.js",
    "map": "node ./bin/generate-maps.js",
    "start": "node ./bin/server.js",
    "build": "npm run update && npm run build-markup && npm run client",
    "build-client": "npm run build-markup && npm run client",
    "client": "browserify ./client/index.js > ./client/index-compiled.js",
    "watch": "watchify ./client/index.js -o ./client/index-compiled.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lnug/lnug.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lnug/lnug.github.io/issues"
  },
  "homepage": "https://github.com/lnug/lnug.github.io",
  "devDependencies": {
    "async": "^1.3.0",
    "chai": "^3.2.0",
    "html-entities": "^1.1.3",
    "imagemagick-stream": "^4.0.1",
    "jquery": "^2.2.4",
    "moment": "^2.10.3",
    "node-html-encoder": "0.0.2",
    "shortid": "^2.2.4",
    "speclate": "4.x.x",
    "speclate-router": "2.x.x",
    "speclate-fetch-markdown": "1.x.x",
    "standard": "^5.4.1",
    "superagent": "^1.3.0",
    "ga-browser": "^1.0.0",
    "appcache-nanny": "^1.1.0"
  },
  "dependencies": {
    "serve-me": "^0.8.2"
  },
  "standard": {
    "globals": [
      "$",
      "location"
    ],
    "ignore": [
      "app-cache-nanny.js",
      "client/index-compiled.js"
    ]
  }
}
