{
  "name": "lnug.github.io",
  "version": "1.0.0",
  "description": "lnug.github.io",
  "main": "build.js",
  "scripts": {
    "test": "standard && mocha --recursive test",
    "update": "node ./bin/update-json.js && npm run fetch-markdown",
    "build-markup": "node ./bin/build-markup.js",
    "fetch-markdown": "node ./bin/fetch-markdown.js",
    "photos": "node ./bin/photos.js",
    "map": "node ./bin/generate-maps.js",
    "start": "speclate --debug 5001",
    "make": "npm run client && speclate --all",
    "build": "npm run update && npm run make",
    "client": "mkdir -p ./docs/client && browserify  ./client/index.js > ./docs/client/index-compiled.js  && npm run service-worker",
    "watch": "watchify ./client/index.js -o ./docs/client/index-compiled.js && watchify ./docs/client/service-worker.js -o ./service-worker.js",
    "service-worker": "browserify ./client/service-worker.js > ./docs/service-worker.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lnug/lnug.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lnug/lnug.github.io/issues"
  },
  "homepage": "https://github.com/lnug/lnug.github.io",
  "devDependencies": {
    "async": "^1.3.0",
    "chai": "^3.2.0",
    "html-entities": "^1.1.3",
    "imagemagick-stream": "^4.0.1",
    "jquery": "^2.2.4",
    "moment": "^2.10.3",
    "node-html-encoder": "0.0.2",
    "shortid": "^2.2.4",
    "speclate": "7.x.x",
    "speclate-router": "4.x.x",
    "appcache-nanny": "^1.1.0",
    "speclate-service-worker": "1.x.x",
    "speclate-fetch-markdown": "1.x.x",
    "standard": "^5.4.1",
    "superagent": "^1.3.0",
    "ga-browser": "^1.0.0"
  },
  "dependencies": {
  },
  "standard": {
    "globals": [
      "$",
      "location",
      "ga"
    ],
    "ignore": [
      "docs",
      "app-cache-nanny.js",
      "client/index-compiled.js"
    ]
  }
}
